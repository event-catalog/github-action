{
  "name": "@eventcatalog/github-action",
  "version": "0.0.1",
  "description": "A GitHub Action for EventCatalog governance",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build src/index.ts -o dist --source-map --license licenses.txt",
    "test": "vitest",
    "format": "prettier --config .prettierrc --write \"**/*.{js,jsx,ts,tsx,json,astro}\"",
    "format:diff": "prettier --config .prettierrc --list-different \"**/*.{js,jsx,ts,tsx,json,astro}\"",
    "changeset": "changeset",
    "release": "changeset publish"
  },
  "keywords": [
    "github",
    "actions",
    "eventcatalog",
    "governance"
  ],
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "action.yml"
  ],
  "author": "EventCatalog",
  "license": "BSL-1.1",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^6.0.0",
    "@ai-sdk/anthropic": "^1.2.12",
    "@ai-sdk/google": "^1.2.18",
    "@ai-sdk/openai": "^1.3.22",
    "@eventcatalog/github-action": "link:",
    "@eventcatalog/sdk": "^2.6.1",
    "ai": "^4.3.16",
    "https-proxy-agent": "^7.0.6",
    "mustache": "^4.2.0",
    "node-fetch": "^3.3.2",
    "zod": "^3.25.48"
  },
  "devDependencies": {
    "@changesets/cli": "^2.28.1",
    "@types/mustache": "^4.2.6",
    "@types/node": "^20.12.0",
    "@vercel/ncc": "^0.38.1",
    "prettier": "^3.5.3",
    "ts-jest": "^29.1.0",
    "typescript": "^5.4.5",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.4"
  }
}
